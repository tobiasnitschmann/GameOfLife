@model GameOfLife.Models.Board 
@{
    ViewData["Title"] = "Game of Life";
}
<div id="gameoflife">
    <table>
        @for (int i = 0; i < Model.width; i++)
        {
            <tr>
                @for (int j = 0; j < Model.height; j++)
                {
                    <td class="@(Model.items[i, j].alive ? "alive": "dead")"></td>
                }
            </tr>
        }

    </table>
</div>
